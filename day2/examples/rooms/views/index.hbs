<section>
  <p>Welcome! Select a Room to join.</p>
  <button id="room1">Join Room 1</button>
  <button id="room2">Join Room 2</button>
  <button id="poke">Poke!</button>
</section>
<script src="/socket.io/socket.io.js"></script>
<script>
  var socket = io();
  var room1="room1";
  var room2="room2";
  socket.on('connect', function() {
    console.log('Connected!');
  });
  $('#room1').on('click',function(){
    socket.emit('enter', "room1")
  });
  $('#room2').on('click',function(){
    socket.emit('enter', "room2")
  });
  $('#poke').on('click',function(){
    socket.emit('poke')
  });
  socket.on('join',function(msg){
    $('section').append('<li style="color:blue">'+msg+'</li>')
  });
  socket.on('poke',function(id){
    $('section').append('<li style="color:red">'+id+" has poked!"+'</li>')
  })
</script>
