<h2 class="center">Logged in as {{user.username}}</h2>
<h3>All Products</h3>
<table class="table table-hover">

  <tbody>
{{#each products}}
  <tr>
    <td>{{this.title}}</td>
    <td>{{this.description}}</td>
    <td><img src="{{imageUri}}" style="max-width:20%"></td>
  </tr>
{{/each}}
  </tbody>
</table>
