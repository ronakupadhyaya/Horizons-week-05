{{! YOUR CODE HERE }}

<h3>Restaurants</h3>
<table class="table table-hover">
  <thead>
    <td><b>Name</b></td>
    <td><b>price</b></td>
    <td><b>Category</b></td>
    <td><b>openTime</b></td>
    <td><b>closingTime</b></td>
    <td><b>Links</b></td>
  </thead>
  <tbody>

  <h3> Page: {{page}} </h3>
	<a href="/restaurants/lists{{prev}}" class="btn btn-default">Previous Page</a>
	<a href="/restaurants/lists{{next}}" class="btn btn-default">Next Page</a>
    {{#each restaurants}}
    <tr>
      <td>{{name}}</td>
      <td>{{price}}</td>
      <td>{{category}}</td>
      <td>{{openTime}}</td>
      <td>{{closingTime}}</td>
      <td>
        <a href="/restaurants/{{id}}">Restaurant Page</a>
      </td>
    </tr>
    {{/each}}
  </tbody>
</table>
    <a href="/restaurants/new">New Restaurant</a>
