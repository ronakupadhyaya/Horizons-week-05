<h1>Profiles</h1>
{{#each users}}
<div class = "box">
	<div class="displayName">Username: {{this.displayName}}</div>
	<div class="email">Email: {{this.email}}</div>
	<div class="location">{{location}}</div>
	<a href="/profile/{{this._id}}">View full profile</a>
</div>
{{/each}}
<a href="/logout">logout</a>